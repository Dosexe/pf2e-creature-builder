{
  "name": "foundryvtt-pf2e-creature-builder",
  "version": "1.0.0",
  "description": "A module that allows you to make an NPC/Monster from scratch using Creature Building Rules.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@league-of-foundry-developers/foundry-vtt-types": "^9.280.0",
    "typescript": "^4.9.5",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@yaireo/tagify": "^4.16.4"
  }
}
